@model VideoViewModel

@{
    ViewData["Title"] = "Match Database";
}

<div class="panel panel-default todo-panel">
    <div class="panel-heading">@ViewData["Title"]</div>

    <table class="table table-hover">
        <thead>
            <tr>
                <td>VideoLink;</td>
                <td>Player1</td>
                <td>Player2</td>
            </tr>
        </thead>

        @foreach (var video in Model.Videos)
      {
        
        <tr>
                 <td>
                                <a href=@video.TimeStamp>&#9876</a>
                    </td>
            <td>@video.Player1</td>
            <td>@video.Player2</td>
                
        </tr>
        
        
      }
    </table>

    <div class="panel-footer add-video-form">
        @await Html.PartialAsync("AddVideoPartial", new VideoLink())
    </div>
</div>